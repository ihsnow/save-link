{
  "manifest_version": 3,
  "name": "Save Link",
  "version": "1.1.0",

  "description": "Downloads a link in a browser agnostic format and closes the tab",

  "action": {},

  "icons": {
    "48": "icon128.png",
    "128": "icon512.png"
  },

  "permissions": ["activeTab", "contextMenus", "scripting"],

  "background": {
    "service_worker": "script.js"
  }
}
